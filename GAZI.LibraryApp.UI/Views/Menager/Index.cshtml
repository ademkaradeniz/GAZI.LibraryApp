
@{
    ViewBag.Title = "Anasayfa";
    Layout = "~/Views/Shared/_MenagerLayout.cshtml";
}

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">En Çok Tercih Edilen Kurslar </font></font></h2>
                <ul class="nav navbar-right panel_toolbox">
                    <li style="float:right;">
                        <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                </ul>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <div id="datatable_wrapper" class="dataTables_wrapper form-inline dt-bootstrap no-footer">
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <table id="datatable" class="table table-striped table-bordered dataTable no-footer" role="grid" aria-describedby="datatable_info">
                                <thead>
                                    <tr>
                                        <th><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Sıra</font></font></th>
                                        <th><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Kurs Adı</font></font></th>
                                        <th><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Başlama - Bitiş Tarih</font></font></th>
                                        <th><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Başlama - Bitiş Saat</font></font></th>
                                        <th><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Yer</font></font></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @{
                                        @*int sira = 1;
                                        foreach (var item in null)
                                        {
                                            if (sira <= 10)
                                            {
                                                <tr>
                                                    <th scope="row" style="padding-left:15px;"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">@sira</font></font></th>
                                                    <td><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">@item.KursAdi</font></font></td>
                                                    <td><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">@item.BaslamaTarih.ToString("dd.MM.yyyy") - @item.BitisTarih.ToString("dd.MM.yyyy")</font></font></td>
                                                    <td><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">@item.BaslamaSaat - @item.BitisSaat</font></font></td>
                                                    <td><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">@item.Yer</font></font></td>
                                                </tr>
                                                sira++;
                                            }
                                            else
                                            {
                                                break;
                                            }

                                        }*@
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>

    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Yeni Açılan Kurslar </font></font></h2>
                <ul class="nav navbar-right panel_toolbox">
                    <li style="float:right;">
                        <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                </ul>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <div id="datatable_wrapper" class="dataTables_wrapper form-inline dt-bootstrap no-footer">
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <table id="datatable-fixed-header" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Sıra</font></font></th>
                                        <th><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Kurs Adı</font></font></th>
                                        <th><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Başlama - Bitiş Tarih</font></font></th>
                                        <th><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Başlama - Bitiş Saat</font></font></th>
                                        <th><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Yer</font></font></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @{
                                        @*int ysira = 1;
                                        foreach (var item in null)
                                        {
                                            if (ysira <= 10)
                                            {
                                                <tr>
                                                    <th scope="row" style="padding-left:15px;"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">@ysira</font></font></th>
                                                    <td><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">@item.KursAdi</font></font></td>
                                                    <td><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">@item.BaslamaTarih.ToString("dd.MM.yyyy") - @item.BitisTarih.ToString("dd.MM.yyyy")</font></font></td>
                                                    <td><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">@item.BaslamaSaat - @item.BitisSaat</font></font></td>
                                                    <td><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">@item.Yer</font></font></td>
                                                </tr>
                                                ysira++;
                                            }
                                            else
                                            {
                                                break;
                                            }

                                        }*@
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>

